<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8">
  <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">

  <!-- title -->
  <% var title=page.title; if ( title=='' ) { var title=page.slug.split("/")[page.slug.split("/").length-1] }else { var
    title=page.title; }; %>
  <title>
    <% if (title){ %>
    <%= title %> |
    <% } %>
    <%= config.title %>
  </title>

  <!-- keywords,description -->
  <% if (page.keywords){ %>
  <meta name="keywords" content="<%= page.keywords %>,<%= config.keywords %>" />
  <% } else if (config.keywords){ %>
  <meta name="keywords" content="<%= config.keywords %>" />
  <% } %>
  <% if (page.description){ %>
  <meta name="description" content="<%= page.description %>" />
  <% } else if (config.description){ %>
  <meta name="description" content="<%= config.description %>" />
  <% } %>

  <!-- favicon -->
  <% if (theme.favicon){ %>
  <%- favicon_tag(theme.favicon) %>
  <% } %>

  <!-- scripts -->

  <%- css('css/main.css') %>
  <%- css('https://cdn.staticfile.org/font-awesome/4.7.0/css/font-awesome.min.css') %>

  <%- js('https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js') %>
  <%- js('https://cdnjs.cloudflare.com/ajax/libs/jquery.pjax/2.0.1/jquery.pjax.min.js') %>

  <%- css('https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.9.0/styles/a11y-dark.min.css')%>

  <%- js('js/main.js') %>
</head>